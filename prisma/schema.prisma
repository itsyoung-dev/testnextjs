generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider          = "postgresql"
    url               = env("DATABASE_URL")
    shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model User {
    id       String  @id @unique
    username String  @unique
    name     String
    image    String?
    bio      String?

    onboarded Boolean @default(false)
}
